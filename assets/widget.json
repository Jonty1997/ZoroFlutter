{
  "Container": {
    "name": "Container",
    "nameCN": "容器",
    "desc": "一个方便的小部件,可以容纳单个子组件",
    "level": 5,
    "info": [
      "【alignment】：设置child的对齐方式 [AlignmentGeometry]",
      "【padding】：与child的内边距",
      "【color】：设置容器的背景色",
      "【decoration】：child后面的装饰",
      "【foregroundDecoration】：child前面的装饰",
      "【width】：宽度",
      "【height】：高度",
      "【constraints】：设置对子属性的约束",
      "【margin】：container的外边距",
      "【transform】：转换矩阵",
      "【child】：子控件"
    ]
  },
  "AboutDialog": {
    "name": "AboutDialog",
    "nameCN": "简介弹窗",
    "desc": "这是一个对话框，其中包含应用程序的图标，名称，版本号和版权，以及用于显示该应用程序使用的软件的许可证的按钮。",
    "level": 2,
    "info": [
      "【applicationName】：显示APP的名称（自己会获取）",
      "【applicationVersion】：APP的版本",
      "【applicationIcon】：APP的icon",
      "【applicationLegalese】：小字号的字符串，用于说明",
      "【children】：子组件列表"
    ]
  },
  "AboutListTile": {
    "name": "AboutListTile",
    "nameCN": "关于应用条目",
    "desc": "一个点击条目，点击弹出AboutDialog(包含应用信息)",
    "level": 2,
    "info": [
      "【icon】：图标",
      "【child】：子组件",
      "【applicationName】：显示APP的名称",
      "【applicationVersion】：APP的版本",
      "【applicationIcon】：APP的icon",
      "【applicationLegalese】：小字号的字符串，用于说明",
      "【aboutBoxChildren】：弹窗内的内容组件"
    ]
  },
  "ActionChip": {
    "name": "ActionChip",
    "nameCN": "事件小条目",
    "desc": "一个事件小条目，有点击的属性，可以指定点击事件以及点击事的样式",
    "level": 2,
    "info": [
      "【avatar】：头像子组件（左边）",
      "【label】：文字子组件（中间）",
      "【onPressed】：点击事件",
      "【shadowColor】：阴影颜色",
      "【elevation】：影深"
    ]
  },
  "AlertDialog": {
    "name": "AlertDialog",
    "nameCN": "警告弹窗",
    "desc": "警告对话框通知用户需要确认的情况，具有可选的标题、内容以及动作组件",
    "level": 3,
    "info": [
      "【title】：标题",
      "【content】：内容",
      "【actions】：底部动作组件",
      "【elevation】：影深",
      "【shape】：形状"
    ]
  },
  "AnimatedIcon": {
    "name": "AnimatedIcon",
    "nameCN": "动画图标",
    "desc": "用动画控制器control可以使得图标进行动画变换",
    "level": 3,
    "info": [
      "【icon】：动画图标数据",
      "【progress】：动画",
      "【color】：颜色",
      "【size】：大小"
    ]
  },
  "BackButton": {
    "name": "BackButton",
    "nameCN": "返回按钮",
    "desc": "一个material风格的返回按钮",
    "level": 1,
    "info": [
      "【color】：图标颜色",
      "【onPressed】：点击事件"
    ]
  },
  "Banner": {
    "name": "Banner",
    "nameCN": "横幅标语",
    "desc": "一个横幅，用于说明，类似我们测试时候应用右上角的debug标签",
    "level": 1,
    "info": [
      "【message】：横幅内的文字",
      "【location】：横幅相对位置",
      "【color】：横幅颜色"
    ]
  },
  "BottomSheet": {
    "name": "BottomSheet",
    "nameCN": "底部抽屉",
    "desc": "该小组件很少直接使用，通常都是以[ScaffoldState.showBottomSheet]或者[showModalBottomSheet]从底部弹出",
    "level": 4,
    "info": [
      "【onClosing】：关闭回调",
      "【builder】：组件构造器",
      "【shape】：形状",
      "【backgroundColor】：背景颜色"
    ]
  },
  "Builder": {
    "name": "Builder",
    "nameCN": "组件构造器",
    "desc": "同一个类中使用`XXX.of(context)`获取某类状态对象方法会存在`上下文滞后`的错误，使用Builder解决。",
    "level": 1,
    "info": [
      "【onClosing】：关闭回调",
      "【builder】：组件构造器",
      "【shape】：形状",
      "【backgroundColor】：背景颜色"
    ]
  }
}
